# Configuration du serveur
PORT=3000
NODE_ENV=production

# URL autorisée pour CORS (votre site web)
# En production, remplacer par votre domaine réel (ex: https://votresite.com)
ALLOWED_ORIGIN=http://localhost

# Clés EmailJS
# Obtenir les clés depuis: https://dashboard.emailjs.com/admin
EMAILJS_PUBLIC_KEY=votre_cle_publique
EMAILJS_PRIVATE_KEY=votre_cle_privee
EMAILJS_SERVICE_ID=votre_service_id
EMAILJS_TEMPLATE_ID=votre_template_id

# Email destinataire
RECIPIENT_EMAIL=votre@email.com

# Clé secrète reCAPTCHA
# Obtenir depuis: https://www.google.com/recaptcha/admin
RECAPTCHA_SECRET_KEY=votre_cle_secrete_recaptcha
